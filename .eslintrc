{
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "Bloodhound": true,
    "gettext": true
  },
  "extends": "eslint:recommended",
  "rules": {
    // Possible Errors
    "no-extra-parens": [
      1,
      "all"
    ],
    // Best Practices
    "accessor-pairs": [
      1,
      {
        "getWithoutSet": true,
        "setWithoutGet": true
      }
    ],
    "array-callback-return": 2,
    "block-scoped-var": 1,
    "complexity": [
      1,
      20
    ],
    "consistent-return": 1,
    "curly": [
      1,
      "all"
    ],
    "default-case": 1,
    "dot-notation": 1,
    "dot-location": [
      1,
      "property"
    ],
    "eqeqeq": [
      1,
      "smart"
    ],
    "no-alert": 1,
    "no-caller": 1,
    "no-case-declarations": 2,
    "no-div-regex": 1,
    "no-else-return": 1,
    "no-empty-function": 2,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": [
      1,
      {
        "boolean": true,
        "number": true,
        "string": true
      }
    ],
    "no-implicit-globals": 0,
    "no-implied-eval": 1,
    // Can be enable later
    "no-invalid-this": 0,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-magic-numbers": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new": 2,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-param-reassign": [
      1,
      {
        "props": false
      }
    ],
    "no-proto": 1,
    "no-return-assign": [
      1,
      "always"
    ],
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-useless-concat": 2,
    "no-void": 1,
    "no-warning-comments": 1,
    "no-with": 1,
    "radix": 1,
    "vars-on-top": 0,
    "wrap-iife": [
      1,
      "inside"
    ],
    "yoda": [
      1,
      "never",
      {}
    ],
    // Strict
    "strict": [
      1,
      "global"
    ],
    // Variables
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-label-var": 2,
    "no-restricted-globals": 0,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-use-before-define": 2,
    // Style
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "eol-last": 2,
    "func-names": 0,
    "func-style": [
      2,
      "declaration"
    ],
    "id-blacklist": 2,
    "id-length": [
      "error",
      {
        "exceptions": [
          "x",
          "y",
          "z",
          "i",
          "j",
          "k"
        ]
      }
    ],
    "id-match": 2,
    "indent": [
      "error",
      4
    ],
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": [
      "error",
      "unix"
    ],
    "lines-around-comment": 2,
    "max-depth": 2,
    "max-len": [
      "error",
      100,
      4
    ],
    "max-nested-callbacks": 10,
    "max-params": 10,
    "max-statements": 10,
    "new-cap": 10,
    "new-parens": 10,
    "newline-after-var": 0,
    "newline-before-return": 10,
    "newline-per-chained-call": 10,
    "no-array-constructor": 10,
    "no-bitwise": 10,
    "no-continue": 10,
    "no-inline-comments": 10,
    "no-lonely-if": 10,
    "no-mixed-spaces-and-tabs": 10,
    "no-multiple-empty-lines": 10,
    "no-negated-condition": 10,
    "no-nested-ternary": 10,
    "no-new-object": 10,
    "no-plusplus": 10,
    "no-restricted-syntax": 10,
    "no-spaced-func": 10,
    "no-ternary": 10,
    "no-trailing-spaces": 10,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 10,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": 2,
    "one-var": [
      "error",
      "never"
    ],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "padded-blocks": [
      "error",
      "never"
    ],
    "quote-props": [
      "error",
      "as-needed"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "require-jsdoc": 0,
    "semi": [
      "error",
      "always"
    ],
    "semi-spacing": 2,
    "sort-imports": 2,
    "sort-vars": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "wrap-regex": 2
  }
}